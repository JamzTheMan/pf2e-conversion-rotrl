ll perfectly sensible life choices.</p>","privateNotes":""},"traits":{"value":["goblin","humanoid"],"senses":{"value":"darkvision"},"languages":{"value":["common","goblin"],"selected":[],"custom":""},"rarity":"common","size":{"value":"sm"},"attitude":{"value":"hostile"}},"resources":{"focus":{"max":1,"value":1}},"schema":{"version":0.813,"lastMigration":{"datetime":"2023-01-11T11:04:34.869-06:00","version":{"schema":0.719,"foundry":"10.291","system":"4.6.5"}}},"abilities":{"str":{"mod":2},"dex":{"mod":3},"con":{"mod":2},"int":{"mod":1},"wis":{"mod":0},"cha":{"mod":4}},"saves":{"fortitude":{"value":7,"saveDetail":""},"reflex":{"value":8,"saveDetail":""},"will":{"value":5,"saveDetail":""}}},"ownership":{"default":0,"USyNCCBYkl6altwM":3},"prototypeToken":{"disposition":-1,"height":1,"name":"Goblin War Chanter","width":1,"displayName":20,"actorLink":false,"lockRotation":false,"rotation":0,"alpha":1,"light":{"alpha":0.5,"angle":360,"bright":0,"coloration":1,"dim":0,"luminosity":0.5,"saturation":0,"contrast":0,"shadows":0,"animation":{"speed":5,"intensity":5,"reverse":false,"type":null},"darkness":{"min":0,"max":1},"color":null,"attenuation":0.5},"displayBars":20,"bar1":{"attribute":"attributes.hp"},"bar2":{"attribute":null},"flags":{"pf2e":{"linkToActorSize":true,"autoscale":true},"perfect-vision":{"visionRules":"default","monoVisionColor":"","sightLimit":null}},"randomImg":false,"texture":{"src":"modules/pf2e-conversion-rotrl/images/sihedron-rune.webp","tint":null,"scaleX":0.8,"scaleY":0.8,"offsetX":0,"offsetY":0,"rotation":0},"sight":{"angle":360,"enabled":true,"range":0,"brightness":1,"visionMode":"basic","color":null,"attenuation":0.1,"saturation":0,"contrast":0},"detectionModes":[]},"_stats":{"systemId":"pf2e","systemVersion":"4.6.5","coreVersion":"10.291","createdTime":1673456676153,"modifiedTime":1673457000860,"lastModifiedBy":"ZFGymRexiCVeiXzz"}}
