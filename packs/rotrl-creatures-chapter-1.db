ions":{"value":2},"requirements":{"value":""},"trigger":{"value":""},"deathNote":false,"weapon":{"value":""}},"ownership":{"default":0},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}},{"_id":"tMhjT7tf9r9vZX4C","effects":[],"flags":{},"img":"systems/pf2e/icons/actions/Passive.webp","name":"Vargouille Transformation","sort":500000,"type":"action","folder":null,"system":{"description":{"value":"<p data-visibility=\"gm\">Exposing the victim to sunlight or any light spell of 3rd level or higher pauses the progression of the disease until the victim is no longer in light.</p>\n<p><strong>Saving Throw</strong> @Check[type:fortitude|dc:18]</p>\n<p data-visibility=\"gm\"><strong>Stage 1</strong> carrier with no effect ([[/br 1d6 #hours]] hours);</p>\n<p data-visibility=\"gm\"><strong>Stage 2</strong> victim's hair falls out ([[/br 1d6 #hours]] hours);</p>\n<p data-visibility=\"gm\"><strong>Stage 3</strong> victim's ears grow into leathery wings, tentacles sprout on the chin and scalp, and the teeth become long, pointed fangs (1 day);</p>\n<p data-visibility=\"gm\"><strong>Stage 4</strong> victim's mind regresses to a vicious and unintelligent shell of its former self ([[/br 1d6 #hours]] hours);</p>\n<p data-visibility=\"gm\"><strong>Stage 5</strong> victim's head breaks free of the body (which dies) and becomes a new vargouille</p>"},"source":{"value":""},"traits":{"value":["disease"],"rarity":"common","custom":""},"rules":[],"slug":null,"schema":{"version":0.813,"lastMigration":{"datetime":"2023-01-11T11:04:35.510-06:00","version":{"schema":0.719,"foundry":"10.291","system":"4.6.5"}}},"actionType":{"value":"passive"},"actionCategory":{"value":"offensive"},"actions":{"value":null},"requirements":{"value":""},"trigger":{"value":""},"deathNote":false,"weapon":{"value":""}},"ownership":{"default":0},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":