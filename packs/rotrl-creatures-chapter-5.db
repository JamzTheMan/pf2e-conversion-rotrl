nd)</p>"},"source":{"value":""},"traits":{"value":["poison"],"rarity":"common","custom":""},"rules":[],"slug":null,"schema":{"version":0.73,"lastMigration":null},"actionType":{"value":"passive"},"actionCategory":{"value":"offensive"},"actions":{"value":null},"requirements":{"value":""},"trigger":{"value":""},"weapon":{"value":""}},"effects":[],"flags":{},"img":"systems/pf2e/icons/actions/Passive.webp","name":"Rupturing Venom","sort":2100000,"type":"action","folder":null,"permission":{"default":0}},{"_id":"y6kXieBaqCizc9RC","data":{"description":{"value":""},"source":{"value":""},"traits":{"value":[],"rarity":"common","custom":""},"rules":[],"slug":null,"schema":{"version":0.73,"lastMigration":null},"mod":{"value":22},"proficient":{"value":0}},"effects":[],"flags":{},"img":"systems/pf2e/icons/default-icons/lore.svg","name":"Acrobatics","sort":2200000,"type":"lore","folder":null,"permission":{"default":0}},{"_id":"VXXutn5ymEFtPpj0","data":{"description":{"value":""},"source":{"value":""},"traits":{"value":[],"rarity":"common","custom":""},"rules":[],"slug":null,"schema":{"version":0.73,"lastMigration":null},"mod":{"value":25},"proficient":{"value":0}},"effects":[],"flags":{},"img":"systems/pf2e/icons/default-icons/lore.svg","name":"Athletics","sort":2300000,"type":"lore","folder":null,"permission":{"default":0}},{"_id":"epzTH6XFReom4LNy","data":{"description":{"value":""},"source":{"value":""},"traits":{"value":[],"rarity":"common","custom":""},"rules":[],"slug":null,"schema":{"version":0.73,"lastMigration":null},"mod":{"value":25},"proficient":{"value":0}},"effects":[],"flags":{},"img":"systems/pf2e/icons/default-icons/lore.svg","name":"Intimidation","sort":2400000,"type":"lore","folder":null,"permission":{"default":0}},{"_id":"uXNJtDgI65Jox7sM","data":{"description":{"value":""},"source":{"value":""},"traits":{"value":[],"rarity":"common","custom":""},"rules":[],"slug":null,"schema":{"version":0.73,"lastMigration":null},"mod":{"value":22},"proficient":{"value":0}},"