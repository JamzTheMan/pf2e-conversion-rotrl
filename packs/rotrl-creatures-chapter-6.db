ance bonus to checks to identify evocation magic</li>\n</ul>"},"source":{"value":"Pathfinder Core Rulebook"},"traits":{"value":["evocation","magical","staff","two-hand-d8"],"rarity":"common","custom":""},"rules":[{"key":"FlatModifier","label":"Staff of Evocation (identify evocation magic)","predicate":{"all":["action:identify-magic","evocation"]},"selector":"skill-check","type":"circumstance","value":2}],"slug":"staff-of-evocation-major","schema":{"version":0.719,"lastMigration":null},"quantity":{"value":1},"baseItem":"staff","hp":{"value":0},"maxHp":{"value":0},"hardness":{"value":0},"brokenThreshold":{"value":0},"weight":{"value":"1"},"equippedBulk":{"value":""},"price":{"value":"4,000 gp"},"equipped":{"carryType":"worn","handsHeld":0},"stackGroup":{"value":""},"bulkCapacity":{"value":""},"negateBulk":{"value":"0"},"containerId":{"value":""},"preciousMaterial":{"value":""},"preciousMaterialGrade":{"value":""},"collapsed":{"value":false},"size":{"value":"med"},"identification":{"status":"identified","unidentified":{"name":"","img":"","data":{"description":{"value":""}}},"misidentified":{}},"usage":{"value":"held-in-one-hand"},"level":{"value":14},"invested":{"value":false},"category":"simple","group":"club","hands":{"value":""},"bonus":{"value":0},"damage":{"value":"","dice":1,"die":"d4","damageType":"bludgeoning"},"bonusDamage":{"value":0},"splashDamage":{"value":0},"range":null,"reload":{"value":""},"MAP":{"value":""},"potencyRune":{"value":null},"strikingRune":{"value":""},"specific":{"value":false},"propertyRune1":{"value":""},"propertyRune2":{"value":""},"propertyRune3":{"value":""},"propertyRune4":{"value":""},"property1":{"value":"","dice":null,"die":"","damageType":"","critDice":null,"critDie":"","critDamage":"","critDamageType":"","criticalConditionType":"","criticalConditionValue":null,"strikeConditionType":"","strikeConditionValue":null}},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"0kbENmzMft491rsv":3},"flags":{"core":{"sourceId":"Comp