:false,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":"1 object of 1 Bulk or less, either unattended or possessed by you or a willing ally"},"range":{"value":"touch"},"area":{"value":"","areaType":""},"time":{"value":"2"},"duration":{"value":"until the next time you make your daily preparations"},"damage":{"value":{}},"save":{"value":"","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"R2faI16Wfucg8NK4"},"hasCounteractCheck":{"value":false},"autoHeightenLevel":{"value":null},"areasize":{"value":""},"prepared":{"value":""},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""}},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"0kbENmzMft491rsv":3},"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.WBmvzNDfpwka3qT4"}}},{"_id":"Ybxo8f39cCyQzSBc","name":"Stabilize","type":"spell","img":"systems/pf2e/icons/spells/stabelize.webp","data":{"description":{"value":"<p>Positive energy shuts death's door. The target loses the @Compendium[pf2e.conditionitems.yZRUzMqrMmfLu0V1]{Dying} condition, though it remains @Compendium[pf2e.conditionitems.fBnFDH2MTzgFijKf]{Unconscious} at 0 Hit Points.</p>"},"source":{"value":"Pathfinder Core Rulebook"},"traits":{"value":["positive","healing","cantrip"],"rarity":"common","custom":""},"rules":[],"slug":"stabilize","schema":{"version":0.719,"lastMigration":null},"level":{"value":1},"spellType":{"value":"utility"},"category":{"value":"spell"},"traditions":{"value":["divine","primal"],"custom":""},"school":{"value":"necromancy"},"components":{"focus":false,"material":false,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":"1 dying creature"},"range":{"value":"30 feet"},"area":{"value":"","areaType":""},"time":{"value":"2"},"duration":{"value":""},"damage":{"value":{}},"save":{"value":"","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"R2faI16Wfucg8NK4"},"hasCounteractCheck":{"value":false},"autoHeightenLevel":{"value":null},"areasize":{"value":""},"prepared":{"value":""},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""}},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"0kbENmzMft491rsv":3},"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.SnjhtQYexDtNDdEg"}}},{"_id":"D2jNRLn2kmeTQzVw","name":"Read Aura","type":"spell","img":"systems/pf2e/icons/spells/read-aura.webp","data":{"description":{"value":"<p>You focus on the target object, opening your mind to perceive magical auras. When the casting is complete, you know whether that item is magical, and if it is, you learn the school of magic.</p>\n<p>If the object is illusory, you detect this only if the effect's level is lower than the level of your read aura spell.</p>\n<hr />\n<p><strong>Heightened (3rd)</strong> You can target up to 10 objects.</p>\n<p><strong>Heightened (6th)</strong> You can target any number of objects.</p>"},"source":{"value":"Pathfinder Core Rulebook"},"traits":{"value":["detection","cantrip"],"rarity":"common","custom":""},"rules":[],"slug":"read-aura","schema":{"version":0.719,"lastMigration":null},"level":{"value":1},"spellType":{"value":"utility"},"category":{"value":"spell"},"traditions":{"value":["arcane","divine","occult","primal"],"custom":""},"school":{"value":"divination"},"components":{"focus":false,"material":false,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":"1 object"},"range":{"value":"30 feet"},"area":{"value":null,"areaType":""},"time":{"value":"1 Minute"},"duration":{"value":""},"damage":{"value":{}},"save":{"value":"","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"R2faI16Wfucg8NK4"},"hasCounteractCheck":{"value":false},"autoHeightenLevel":{"value":null},"areasize":{"value":""},"prepared":{"value":""},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""}},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"0kbENmzMft491rsv":3},"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.OhD2Z6rIGGD5ocZA"}}},{"_id":"bFVGFZkKz01uWHs4","name":"Mending","type":"spell","img":"systems/pf2e/icons/spells/mending.webp","data":{"description":{"value":"<p>You repair the target item. You restore 5 Hit Points per spell level to the target, potentially removing the @Compendium[pf2e.conditionitems.6dNUvdb1dhToNDj3]{Broken} condition if this repairs it past the item's Broken Threshold. You can't replace lost pieces or repair an object that's been completely destroyed.</p>\n<hr />\n<p><strong>Heightened (2nd)</strong> You can target a non-magical object of 1 Bulk or less.</p>\n<p><strong>Heightened (3rd)</strong> You can target a non-magical object of 2 Bulk or less, or a magical object of 1 Bulk or less.</p>"},"source":{"value":"Pathfinder Core Rulebook"},"traits":{"value":[],"rarity":"common","custom":""},"rules":[],"slug":"mending","schema":{"version":0.719,"lastMigration":null},"level":{"value":1},"spellType":{"value":"utility"},"category":{"value":"spell"},"traditions":{"value":["arcane","divine","occult","primal"]},"school":{"value":"transmutation"},"components":{"focus":false,"material":false,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":"non-magical object of light Bulk or less"},"range":{"value":"touch"},"area":{"value":null,"areaType":""},"time":{"value":"10 minutes"},"duration":{"value":""},"damage":{"value":{}},"save":{"value":"","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"R2faI16Wfucg8NK4"},"hasCounteractCheck":{"value":false},"autoHeightenLevel":{"value":null},"areasize":{"value":""},"prepared":{"value":""},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""},"heightenedLevel":{"value":1}},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"0kbENmzMft491rsv":3},"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.dINQzhqGmIsqGMUY"}}},{"_id":"3c78H5kiDu6Fuupj","name":"Sanctuary","type":"spell","img":"systems/pf2e/icons/spells/sanctuary.webp","data":{"description":{"value":"<p>You ward a creature with protective energy that deters enemy attacks. Creatures attempting to attack the target must attempt a Will save each time. If the target uses a hostile action, the spell ends.</p>\n<hr /><strong>Critical Success</strong> Sanctuary ends.\n<p><strong>Success</strong> The creature can attempt its attack and any other attacks against the target this turn.</p>\n<p><strong>Failure</strong> The creature can't attack the target and wastes the action. It can't attempt further attacks against the target this turn.</p>\n<p><strong>Critical Failure</strong> The creature wastes the action and can't attempt to attack the target for the rest of sanctuary's duration.</p>"},"source":{"value":"Pathfinder Core Rulebook"},"traits":{"value":[],"rarity":"common","custom":""},"rules":[],"slug":"sanctuary","schema":{"version":0.719,"lastMigration":null},"level":{"value":1},"spellType":{"value":"utility"},"category":{"value":"spell"},"traditions":{"value":["divine","occult"]},"school":{"value":"abjuration"},"components":{"focus":false,"material":false,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":"1 creature"},"range":{"value":"touch"},"area":{"value":null,"areaType":""},"time":{"value":"2"},"duration":{"value":"1 minute"},"damage":{"value":{}},"save":{"value":"","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"R2faI16Wfucg8NK4"},"hasCounteractCheck":{"value":false},"autoHeightenLevel":{"value":null},"areasize":{"value":""},"prepared":{"value":""},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""},"heightenedLevel":{"value":1}},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"0kbENmzMft491rsv":3},"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.8xRzLhwGL7Dgy3EZ"}}},{"_id":"9Aoh7kqSZh9J75pd","name":"Silence","type":"spell","img":"systems/pf2e/icons/spells/silence.webp","data":{"description":{"value":"<p>The target makes no sound, preventing creatures from noticing it using hearing or other senses alone. The target can't use sonic attacks, nor can it use actions with the auditory trait. This prevents it from casting spells that include verbal components.</p>\n<hr />\n<p><strong>Heightened (4th)</strong> The spell effect emanates from the touched creature, silencing all sound in or passing through a 10- foot radius and preventing any auditory and sonic effects in the affected area. While within the radius, creatures are subject to the same effects as the target. Depending upon the position of the effect, a creature might notice the lack of sound reaching it (blocking off the noise coming from a party, for example).</p>"},"source":{"value":"Pathfinder Core Rulebook"},"traits":{"value":[],"rarity":"common","custom":""},"rules":[],"slug":"silence","schema":{"version":0.719,"lastMigration":null},"level":{"value":2},"spellType":{"value":"utility"},"category":{"value":"spell"},"traditions":{"value":["divine","occult"]},"school":{"value":"illusion"},"components":{"focus":false,"material":true,"somatic":true,"verbal":false},"materials":{"value":""},"target":{"value":"1 willing creature"},"range":{"value":"touch"},"area":{"value":null,"areaType":""},"time":{"value":"2"},"duration":{"value":"1 minute"},"damage":{"value":{}},"save":{"value":"","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"R2faI16Wfucg8NK4"},"hasCounteractCheck":{"value":false},"autoHeightenLevel":{"value":null},"areasize":{"value":""},"prepared":{"value":""},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""},"heightenedLevel":{"value":2}},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"0kbENmzMft491rsv":3},"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.gIdDLrbswTV3OBJy"}}},{"_id":"9Zyoqpd23VDzMwPV","name":"Deafness","type":"spell","img":"systems/pf2e/icons/spells/deafness.webp","data":{"description":{"value":"<p>The target loses hearing; it must attempt a Fortitude save. The target is then temporarily immune for 1 minute.</p>\n<hr />\n<p><strong>Critical Success</strong> The target is unaffected.</p>\n<p><strong>Success</strong> The target is @Compendium[pf2e.conditionitems.9PR9y0bi4JPKnHPR]{Deafened} for 1 round.</p>\n<p><strong>Failure</strong> The target is Deafened for 10 minutes.</p>\n<p><strong>Critical Failure</strong> The target is Deafened permanently.</p>"},"source":{"value":"Pathfinder Core Rulebook"},"traits":{"value":[],"rarity":"common","custom":""},"rules":[],"slug":"deafness","schema":{"version":0.719,"lastMigration":null},"level":{"value":2},"spellType":{"value":"save"},"category":{"value":"spell"},"traditions":{"value":["arcane","divine","occult","primal"]},"school":{"value":"necromancy"},"components":{"focus":false,"material":false,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":"1 creature"},"range":{"value":"30 feet"},"area":{"value":null,"areaType":""},"time":{"value":"2"},"duration":{"value":""},"damage":{"value":{}},"save":{"value":"fortitude","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"R2faI16Wfucg8NK4"},"hasCounteractCheck":{"value":false},"autoHeightenLevel":{"value":null},"areasize":{"value":""},"prepared":{"value":""},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""},"heightenedLevel":{"value":2}},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"0kbENmzMft491rsv":3},"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.fZPCv2VHuM2yPbC8"}}},{"_id":"7c0p2qm7amQNy1D2","name":"Endure Elements","type":"spell","img":"systems/pf2e/icons/spells/endure-elements.webp","data":{"description":{"value":"<p>You shield the target against dangerous temperatures. Choose severe cold or heat. The target is protected from the temperature you chose (but not extreme cold or heat).</p>\n<hr />\n<p><strong>Heightened (3rd)</strong> The target is protected from severe cold and heat.</p>\n<p><strong>Heightened (5th)</strong> The target is protected from severe cold, severe heat, extreme cold, and extreme heat.</p>"},"source":{"value":"Pathfinder Core Rulebook"},"traits":{"value":[],"rarity":"common","custom":""},"rules":[],"slug":"endure-elements","schema":{"version":0.719,"lastMigration":null},"level":{"value":2},"spellType":{"value":"utility"},"category":{"value":"spell"},"traditions":{"value":["arcane","divine","primal"]},"school":{"value":"abjuration"},"components":{"focus":false,"material":false,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":"1 willing creature"},"range":{"value":"touch"},"area":{"value":null,"areaType":""},"time":{"value":"10 minutes"},"duration":{"value":"until the next time you make your daily preparations"},"damage":{"value":{}},"save":{"value":"","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"R2faI16Wfucg8NK4"},"hasCounteractCheck":{"value":false},"autoHeightenLevel":{"value":null},"areasize":{"value":""},"prepared":{"value":""},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""},"heightenedLevel":{"value":3}},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"0kbENmzMft491rsv":3},"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.5esP2GVzvxWsMgaX"}}},{"_id":"usCQUQLrN8oiGRDP","name":"Agonizing Despair","type":"spell","img":"systems/pf2e/icons/spells/agonizing-despair.webp","data":{"description":{"value":"<p>Your target's mind tumbles down a deep well of dread, dwelling so intently on deep-seated fears that it's painful. The target takes [[/r {4d6}[mental]]]{4d6 mental damage} with a Will saving throw.</p>\n<hr />\n<p><strong>Critical Success</strong> The target is unaffected.</p>\n<p><strong>Success</strong> The target takes half damage and becomes @Compendium[pf2e.conditionitems.TBSHQspnbcqxsmjL]{Frightened 1}.</p>\n<p><strong>Failure</strong> The target takes full damage and becomes @Compendium[pf2e.conditionitems.TBSHQspnbcqxsmjL]{Frightened 2}.</p>\n<p><strong>Critical Failure</strong> The target takes double damage and becomes @Compendium[pf2e.conditionitems.TBSHQspnbcqxsmjL]{Frightened 3}.</p>\n<hr />\n<p><strong>Heightened (+1)</strong> Increase the damage by [[/r {2d6}[mental]]].</p>"},"source":{"value":"Pathfinder Advanced Player's Guide"},"traits":{"value":["emotion","fear","mental"],"rarity":"common","custom":""},"rules":[],"slug":"agonizing-despair","schema":{"version":0.719,"lastMigration":null},"level":{"value":3},"spellType":{"value":"save"},"category":{"value":"spell"},"traditions":{"value":["arcane","divine","occult"],"custom":""},"school":{"value":"enchantment"},"components":{"focus":false,"material":false,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":"1 creature"},"range":{"value":"60 feet"},"area":{"value":"","areaType":""},"time":{"value":"2"},"duration":{"value":""},"damage":{"value":{"0":{"applyMod":false,"type":{"categories":[],"subtype":"","value":"mental"},"value":"4d6"}}},"save":{"value":"will","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"R2faI16Wfucg8NK4"},"hasCounteractCheck":{"value":false},"autoHeightenLevel":{"value":null},"areasize":{"value":""},"prepared":{"value":false},"primarycheck":{"value":""},"scaling":{"damage":{"0":"2d6"},"interval":1},"secondarycasters":{"value":""},"secondarycheck":{"value":""},"heightenedLevel":{"value":3}},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"0kbENmzMft491rsv":3},"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.nplNt08TvokZUxtR"}}},{"_id":"KeYpcg9G7WL7HPXp","name":"Air Walk","type":"spell","img":"systems/pf2e/icons/spells/air-walk.webp","data":{"description":{"value":"<p>The target can walk on air as if it were solid ground. It can ascend and descend in this way at a maximum of a 45-degree angle.</p>"},"source":{"value":"Pathfinder Core Rulebook"},"traits":{"value":["air"],"rarity":"common","custom":""},"rules":[],"slug":"air-walk","schema":{"version":0.719,"lastMigration":null},"level":{"value":4},"spellType":{"value":"utility"},"category":{"value":"spell"},"traditions":{"value":["divine","primal"]},"school":{"value":"transmutation"},"components":{"focus":false,"material":false,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":"1 creature"},"range":{"value":"touch"},"area":{"value":null,"areaType":""},"time":{"value":"2"},"duration":{"value":"5 minutes"},"damage":{"value":{}},"save":{"value":"","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"R2faI16Wfucg8NK4"},"hasCounteractCheck":{"value":false},"autoHeightenLevel":{"value":null},"areasize":{"value":""},"prepared":{"value":""},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""},"heightenedLevel":{"value":4}},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"0kbENmzMft491rsv":3},"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.b5sGjGlBf58f8jn0"}}},{"_id":"0Cths9L4M8eSqCkZ","name":"Resist Energy","type":"spell","img":"systems/pf2e/icons/spells/resist-energy.webp","data":{"description":{"value":"<p>A shield of elemental energy protects a creature against one type of energy damage. Choose acid, cold, electricity, fire, or sonic damage. The target and its gear gain resistance 5 against the damage type you chose.</p>\n<hr />\n<p><strong>Heightened (4th)</strong> The resistance increases to 10, and you can target up to two creatures.</p>\n<p><strong>Heightened (7th)</strong> The resistance increases to 15, and you can target up to five creatures.</p>"},"source":{"value":"Pathfinder Core Rulebook"},"traits":{"value":[],"rarity":"common","custom":""},"rules":[],"slug":"resist-energy","schema":{"version":0.719,"lastMigration":null},"level":{"value":2},"spellType":{"value":"utility"},"category":{"value":"spell"},"traditions":{"value":["arcane","divine","occult","primal"]},"school":{"value":"abjuration"},"components":{"focus":false,"material":false,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":"1 creature"},"range":{"value":"touch"},"area":{"value":null,"areaType":""},"time":{"value":"2"},"duration":{"value":"10 minutes"},"damage":{"value":{}},"save":{"value":"","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"R2faI16Wfucg8NK4"},"hasCounteractCheck":{"value":false},"autoHeightenLevel":{"value":null},"areasize":{"value":""},"prepared":{"value":""},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""},"heightenedLevel":{"value":4}},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"0kbENmzMft491rsv":3},"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.Fr58LDSrbndgld9n"}}},{"_id":"C37rszfk57fMBBI8","name":"Talking Corpse (at will)","type":"spell","img":"systems/pf2e/icons/spells/talking-corpse.webp","data":{"description":{"value":"<p>You grant the target corpse a semblance of life, which it uses to speak the answers to three questions posed to it. This spell calls on the physical body's latent memories rather than summoning back the deceased's spirit, so the corpse must be mostly intact for the spell to function. The more damage the corpse has taken, the more inaccurate or patchwork its answers are, and it must have a throat and mouth to speak at all. If anyone has previously cast this spell on the corpse in the last week, the spell automatically fails. The corpse can attempt a Will save to resist answering the questions using the statistics of the original creature at its time of death, with the following effects.</p>\n<hr /><strong>Critical Success</strong> The target can lie or refuse to answer your questions, and the target's spirit haunts you for 24 hours, bothering you and causing you to be unable to gain any rest for that time.\n<p><strong>Success</strong> The target can provide false information or refuse to answer your questions.</p>\n<p><strong>Failure</strong> The target must answer truthfully, but its answers can be brief, cryptic, and repetitive. It can still mislead you or attempt to stall so that the spell's duration runs out before you can ask all your questions.</p>\n<p><strong>Critical Failure</strong> As failure, but the target's answers are more direct and less repetitive, though still cryptic. It takes a -2 status penalty to Deception checks to deceive or mislead you.</p>"},"source":{"value":"Pathfinder Core Rulebook"},"traits":{"value":[],"rarity":"uncommon","custom":""},"rules":[],"slug":"talking-corpse","schema":{"version":0.719,"lastMigration":null},"level":{"value":4},"spellType":{"value":"save"},"category":{"value":"spell"},"traditions":{"value":["divine","occult"]},"school":{"value":"necromancy"},"components":{"focus":false,"material":true,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":"1 corpse"},"range":{"value":"touch"},"area":{"value":"","areaType":""},"time":{"value":"10 minutes"},"duration":{"value":"10 minutes"},"damage":{"value":{}},"save":{"value":"","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"R2faI16Wfucg8NK4"},"hasCounteractCheck":{"value":false},"autoHeightenLevel":{"value":null},"areasize":{"value":""},"prepared":{"value":""},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""},"heightenedLevel":{"value":4}},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"0kbENmzMft491rsv":3},"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.FM3SmEW8N1FCRjqt"}}},{"_id":"zhmHDVGy3tg7ioZS","name":"Death Ward","type":"spell","img":"systems/pf2e/icons/spells/death-ward.webp","data":{"description":{"value":"<p>You shield a creature from the ravages of negative energy.</p>\n<p>It receives a +4 status bonus to saves against death and negative effects, gains negative resistance 10, and suppresses the effects of the @Compendium[pf2e.conditionitems.3uh1r86TzbQvosxv]{Doomed} condition.</p>\n<p>@Compendium[pf2e.spell-effects.s6CwkSsMDGfUmotn]{Spell Effect: Death Ward}</p>"},"source":{"value":"Pathfinder Core Rulebook"},"traits":{"value":[],"rarity":"common","custom":""},"rules":[],"slug":"death-ward","schema":{"version":0.719,"lastMigration":null},"level":{"value":5},"spellType":{"value":"utility"},"category":{"value":"spell"},"traditions":{"value":["divine","occult","primal"]},"school":{"value":"abjuration"},"components":{"focus":false,"material":false,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":"1 living creature touched"},"range":{"value":"touch"},"area":{"value":"","areaType":""},"time":{"value":"2"},"duration":{"value":"10 minutes"},"damage":{"value":{}},"save":{"value":"","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"R2faI16Wfucg8NK4"},"hasCounteractCheck":{"value":false},"autoHeightenLevel":{"value":null},"areasize":{"value":""},"prepared":{"value":""},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""},"heightenedLevel":{"value":5}},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"0kbENmzMft491rsv":3},"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.YvXKGlHOt7mdW2jZ"}}},{"_id":"zdWGy7JYGLzwZguM","name":"Sending","type":"spell","img":"systems/pf2e/icons/spells/sending.webp","data":{"description":{"value":"<p>You send the creature a mental message of 25 words or fewer, and it can respond immediately with its own message of 25 words or fewer.</p>"},"source":{"value":"Pathfinder Core Rulebook"},"traits":{"value":["mental"],"rarity":"common","custom":""},"rules":[],"slug":"sending","schema":{"version":0.719,"lastMigration":null},"level":{"value":5},"spellType":{"value":"utility"},"category":{"value":"spell"},"traditions":{"value":["arcane","divine","occult"]},"school":{"value":"divination"},"components":{"focus":false,"material":true,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":"1 creature with whom you are familiar"},"range":{"value":"planetary"},"area":{"value":null,"areaType":""},"time":{"value":"3"},"duration":{"value":""},"damage":{"value":{}},"save":{"value":"","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"R2faI16Wfucg8NK4"},"hasCounteractCheck":{"value":false},"autoHeightenLevel":{"value":null},"areasize":{"value":""},"prepared":{"value":""},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""},"heightenedLevel":{"value":5}},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"0kbENmzMft491rsv":3},"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.R9xqCBblkS5KE4y7"}}},{"_id":"Z2SxCaDPi2PVtjp2","name":"Flame Strike","type":"spell","img":"systems/pf2e/icons/spells/rain-of-fire.webp","data":{"description":{"value":"<p>You call a rain of divine fire that plummets down from above, dealing 8d6 fire damage. Because the flame is infused with divine energy, creatures in the area apply only half their usual fire resistance. Creatures that are immune to fire, instead of gaining the usual benefit of immunity, treat the results of their saving throws as one degree of success better.</p>\n<hr />\n<p><strong>Heightened (+1)</strong> The damage increases by 2d6.</p>"},"source":{"value":"Pathfinder Core Rulebook"},"traits":{"value":["fire"],"rarity":"common","custom":""},"rules":[],"slug":"flame-strike","schema":{"version":0.719,"lastMigration":null},"level":{"value":5},"spellType":{"value":"save"},"category":{"value":"spell"},"traditions":{"value":["divine"]},"school":{"value":"evocation"},"components":{"focus":false,"material":false,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":""},"range":{"value":"120 feet"},"area":{"value":null,"areaType":""},"time":{"value":"2"},"duration":{"value":""},"damage":{"value":{"0":{"type":{"categories":[],"value":"fire"},"value":"8d6"}}},"save":{"value":"reflex","basic":"basic"},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"R2faI16Wfucg8NK4"},"hasCounteractCheck":{"value":false},"autoHeightenLevel":{"value":null},"areasize":{"value":"10-foot radius, 40-foot-tall cylinder"},"prepared":{"value":""},"primarycheck":{"value":""},"scaling":{"damage":{"0":"2d6"},"interval":1},"secondarycasters":{"value":""},"secondarycheck":{"value":""},"heightenedLevel":{"value":6}},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"0kbENmzMft491rsv":3},"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.E3X2RbzWHCdz7gsk"}}},{"_id":"lblmLSmPoWXhRoaT","name":"Spiritual Weapon","type":"spell","img":"systems/pf2e/icons/spells/spiritual-weapon.webp","data":{"description":{"value":"<p>A weapon made of pure magical force materializes and attacks foes you designate within range. This weapon has a ghostly appearance and manifests as a club, a dagger, or your deity's favored weapon.</p>\n<p>When you cast the spell, the weapon appears next to a foe you choose within range and makes a Strike against it. Each time you Sustain the Spell, you can move the weapon to a new target within range (if needed) and Strike with it. The spiritual weapon uses and contributes to your multiple attack penalty.</p>\n<p>The weapon's Strikes are melee spell attacks. Regardless of its appearance, the weapon deals force damage equal to 1d8 plus your spellcasting ability modifier. You can deal damage of the type normally dealt by the weapon instead of force damage (or any of the available damage types for a versatile weapon). No other statistics or traits of the weapon apply, and even a ranged weapon attacks adjacent creatures only. Despite making a spell attack, the spiritual weapon is a weapon for purposes of triggers, resistances, and so forth.</p>\n<p>The weapon doesn't take up space, grant flanking, or have any other attributes a creature would. The weapon can't make any attack other than its Strike, and feats or spells that affect weapons do not apply to it.</p>\n<hr />\n<p><strong>Heightened (+2)</strong> The weapon's damage increases by 1d8.</p>"},"source":{"value":"Pathfinder Core Rulebook"},"traits":{"value":["force"],"rarity":"common","custom":""},"rules":[],"slug":"spiritual-weapon","schema":{"version":0.719,"lastMigration":null},"level":{"value":2},"spellType":{"value":"attack"},"category":{"value":"spell"},"traditions":{"value":["divine","occult"]},"school":{"value":"evocation"},"components":{"focus":false,"material":false,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":""},"range":{"value":"120 feet"},"area":{"value":"","areaType":""},"time":{"value":"2"},"duration":{"value":"sustained up to 1 minute"},"damage":{"value":{"0":{"applyMod":true,"type":{"categories":[],"value":"force"},"value":"1d8"}}},"save":{"value":"","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"R2faI16Wfucg8NK4"},"hasCounteractCheck":{"value":false},"autoHeightenLevel":{"value":null},"areasize":{"value":""},"prepared":{"value":""},"primarycheck":{"value":""},"scaling":{"damage":{"0":"1d8"},"interval":2},"secondarycasters":{"value":""},"secondarycheck":{"value":""},"heightenedLevel":{"value":6}},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"0kbENmzMft491rsv":3},"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.Fq9yCbqI2RDt6Orw"}}},{"_id":"njTsn8C6ZzKuJ50H","name":"Blade Barrier","type":"spell","img":"systems/pf2e/icons/spells/blade-barrier.webp","data":{"description":{"value":"<p>Blades of force form a churning wall. The wall is a straight line 20 feet high, 120 feet long, and 2 inches thick, and it provides cover. The wall deals 7d8 force damage to each creature that's in the wall's space when it is created, that attempts to pass through the wall, or that ends its turn inside the wall. A basic Reflex save reduces the damage. A creature that succeeds at this save when the wall is created is pushed to the nearest space on the side of its choice. Creatures trying to move through the wall fail to do so if they critically fail the save, ending their movement adjacent to the wall.</p>\n<hr />\n<p><strong>Heightened (+1)</strong> The damage increases by 1d8.</p>"},"source":{"value":"Pathfinder Core Rulebook"},"traits":{"value":["force"],"rarity":"common","custom":""},"rules":[],"slug":"blade-barrier","schema":{"version":0.719,"lastMigration":null},"level":{"value":6},"spellType":{"value":"utility"},"category":{"value":"spell"},"traditions":{"value":["divine"]},"school":{"value":"evocation"},"components":{"focus":false,"material":true,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":""},"range":{"value":"120 feet"},"area":{"value":null,"areaType":""},"time":{"value":"3"},"duration":{"value":"1 minute"},"damage":{"value":{"0":{"type":{"categories":[],"value":"force"},"value":"7d8"}}},"save":{"value":"reflex","basic":"basic"},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"R2faI16Wfucg8NK4"},"hasCounteractCheck":{"value":false},"autoHeightenLevel":{"value":null},"areasize":{"value":""},"prepared":{"value":""},"primarycheck":{"value":""},"scaling":{"damage":{"0":"1d8"},"interval":1},"secondarycasters":{"value":""},"secondarycheck":{"value":""},"heightenedLevel":{"value":7}},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"0kbENmzMft491rsv":3},"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.peCF6VArm8urfwxZ"}}},{"_id":"zrjA3yWIrIYEE5hL","name":"Ethereal Jaunt","type":"spell","img":"systems/pf2e/icons/spells/etheral-jaunt.webp","data":{"description":{"value":"<p>You travel to the Ethereal Plane, which overlaps the Material Plane. Material Plane creatures can't see you, and you can move through things on the Material Plane. You move at half your normal Speeds, but can move in any direction (including up and down). You can see onto the Material Plane within a radius of 60 feet; it is gray, hazy, and @Compendium[pf2e.conditionitems.DmAIPqOBomZ7H95W]{Concealed} from you. You can't affect the Material Plane, and you can't be affected by the Material Plane except by force effects and abjurations originating there. When the spell ends, you return to the Material Plane. If you're in the air, you fall (unless you can fly), and if you're inside an object, you're pushed into the nearest open space and take 1d6 damage per 5 feet you were pushed. If you cast this spell when not on the Material Plane, it is lost.</p>\n<hr />\n<p><strong>Heightened (9th)</strong> You can target up to five additional willing creatures at a range of 30 feet. The duration is up to 10 minutes.</p>"},"source":{"value":"Pathfinder Core Rulebook"},"traits":{"value":["teleportation"],"rarity":"uncommon","custom":""},"rules":[],"slug":"ethereal-jaunt","schema":{"version":0.719,"lastMigration":null},"level":{"value":7},"spellType":{"value":"utility"},"category":{"value":"spell"},"traditions":{"value":["divine","occult"]},"school":{"value":"conjuration"},"components":{"focus":false,"material":false,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":""},"range":{"value":""},"area":{"value":null,"areaType":""},"time":{"value":"2"},"duration":{"value":"sustained up to 1 minute"},"damage":{"value":{}},"save":{"value":"","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"R2faI16Wfucg8NK4"},"hasCounteractCheck":{"value":false},"autoHeightenLevel":{"value":null},"areasize":{"value":""},"prepared":{"value":""},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""},"heightenedLevel":{"value":7}},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"0kbENmzMft491rsv":3},"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.D2nPKbIS67m9199U"}}},{"_id":"YMM2rgfisbLRDtvo","name":"Sound Burst","type":"spell","img":"systems/pf2e/icons/spells/sound-burst.webp","data":{"description":{"value":"<p>A cacophonous noise blasts out, dealing 2d10 sonic damage. Each creature must attempt a Fortitude save.</p>\n<hr />\n<p><strong>Critical Success</strong> The creature is unaffected.</p>\n<p><strong>Success</strong> The creature takes half damage.</p>\n<p><strong>Failure</strong> The creature takes full damage and is @Compendium[pf2e.conditionitems.9PR9y0bi4JPKnHPR]{Deafened} for 1 round.</p>\n<p><strong>Critical Failure</strong> The creature takes double damage, is Deafened for 1 minute, and is @Compendium[pf2e.conditionitems.dfCMdR4wnpbYNTix]{Stunned 1}.</p>\n<hr />\n<p><strong>Heightened (+1)</strong> The damage increases by 1d10.</p>"},"source":{"value":"Pathfinder Core Rulebook"},"traits":{"value":["sonic"],"rarity":"common","custom":""},"rules":[],"slug":"sound-burst","schema":{"version":0.719,"lastMigration":null},"level":{"value":2},"spellType":{"value":"save"},"category":{"value":"spell"},"traditions":{"value":["divine","occult"]},"school":{"value":"evocation"},"components":{"focus":false,"material":false,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":""},"range":{"value":"30 feet"},"area":{"value":"10","areaType":"burst"},"time":{"value":"2"},"duration":{"value":""},"damage":{"value":{"0":{"type":{"categories":[],"value":"sonic"},"value":"2d10"}}},"save":{"value":"fortitude","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"R2faI16Wfucg8NK4"},"hasCounteractCheck":{"value":false},"autoHeightenLevel":{"value":null},"areasize":{"value":"10-foot burst"},"prepared":{"value":""},"primarycheck":{"value":""},"scaling":{"