ove any number of lights up to 60 feet. Each light must remain within 120 feet of you and within 10 feet of all others, or it winks out.</p>","chat":"","unidentified":"","label":"Description","type":"String"},"source":{"value":"","label":"Source","type":"String"},"traits":{"value":["light","cantrip","evocation"],"rarity":{"value":"common"},"custom":"","label":"Spell Traits","type":"String"},"rules":[],"slug":null,"schema":{"version":0.642,"lastMigration":{"datetime":"2021-07-16T19:43:17.093-05:00","version":{"schema":null,"foundry":"0.8.8","system":"2.9.1.6656"}}},"level":{"value":1,"label":"Spell Level","type":"Number"},"spellType":{"value":"utility","label":"Spell Type","type":"String"},"category":{"value":"spell"},"traditions":{"value":["arcane","occult","primal"],"custom":"","label":"Spell Traditions","type":"String"},"school":{"value":"evocation","label":"Spell School","type":"String"},"components":{"focus":false,"material":false,"somatic":true,"verbal":true,"label":"Spell Components","type":"String"},"materials":{"value":"","label":"Materials","type":"String"},"target":{"value":"","label":"Target","type":"String"},"range":{"value":"120 feet","label":"Range","type":"String"},"area":{"value":null,"areaType":"","label":"Area","type":"String"},"time":{"value":"2","label":"Casting Time","type":"String"},"duration":{"value":"sustained","label":"Duration","type":"String"},"damage":{"value":"","applyMod":false,"label":"Spell Damage","type":"String"},"save":{"value":"","basic":"","label":"Saving Throw","type":"String"},"sustained":{"value":true,"label":"Can be Sustained","type":"Boolean"},"cost":{"value":"","label":"Cost","type":"String"},"ability":{"value":"","label":"Spellcasting Ability","type":"String"},"location":{"value":"4Fk23YGh4jwEuHSj"},"hasCounteractCheck":{"value":false},"autoHeightenLevel":{"value":null},"damageType":{"value":"","label":"Damage Type","type":"String"},"scaling":{"mode":"none","formula":""},"prepared":{"value":"","label":"Prepared Spell","ty